 // Place your key bindings in this file to overwrite the defaults
 [{
     "key": "ctrl+d",
     "command": "editor.action.copyLinesDownAction",
     "when": "editorTextFocus"
 }
 ,{
     "key": "ctrl+e",
     "command": "extension.runQuery",
     "when": "editorTextFocus"
 }
 ,{
     "key": "ctrl+shift+t",
     "command": "shellLauncher.launch"
 }
 ,{ 
     "key": "alt+n",
     "command": "editor.action.insertLineAfter"
 },
 { 
     "key": "alt+d",
     "command": "editor.action.deleteLines"
 },
 { 
     "key": "alt+h",  
     "command": "cursorTop"
 },
 { 
     "key": "alt+e",             
     "command": "cursorBottom"
 },
 {
     "key": "ctrl+[Minus]",
     "command": "editor.action.commentLine",
     "when": "editorTextFocus && !editorReadonly"
 },
 {
     "key": "ctrl+shift+6",
     "command": "-editor.action.commentLine",
     "when": "editorTextFocus && !editorReadonly"
 }
 // Toggle between terminal and editor focus
 ,{ "key": "ctrl+Down", "command": "workbench.action.terminal.focus"},
 { "key": "ctrl+Up", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus"}
 ,
 { "key": "f5",               "command": "workbench.action.debug.run",
     "when": "!inDebugMode" },
 { "key": "ctrl+f5",                    "command": "workbench.action.debug.start",
     "when": "!inDebugMode"
 },
 {
     "key": "ctrl+k",
     "command": "workbench.action.toggleSidebarVisibility"
},

{
    "key": "ctrl+b",
    "command" :"macros.commentDown"
    // ,"when" :"workbench.action.toggleSidebarVisibility.false", 

},
{
    "key": "ctrl+shift+right",
    "command": "workbench.action.splitEditorRight"
},
{
    "key": "ctrl+shift+o",
    "command": "-workbench.action.gotoSymbol"
},
{
    "key": "ctrl+shift+o",
    "command": "workbench.action.files.openFolder"
},
{
    "key": "ctrl+k ctrl+o",
    "command": "-workbench.action.files.openFolder"
},
{
    "key": "alt+o",
    "command": "openInTerminal"
},
{
    "key": "alt+q",
    "command": "workbench.action.terminal.resizePaneDown",
    "when": "terminalFocus"
}
]